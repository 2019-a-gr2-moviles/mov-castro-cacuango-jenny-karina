<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".gestionPeliculas">

    <EditText
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_nombrePelicula1" android:hint="@string/txt_nombrePelicula1"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.492" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.116"/>
    <EditText
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_anioLanzamiento1" android:hint="@string/txt_anioLanzamiento1"
            app:layout_constraintEnd_toEndOf="@+id/txt_nombrePelicula1"
            app:layout_constraintStart_toStartOf="@+id/txt_nombrePelicula1" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_nombrePelicula1" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_rating1"
            android:hint="@string/txt_rating1"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="@+id/txt_anioLanzamiento1"
            android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="@+id/txt_anioLanzamiento1" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_anioLanzamiento1" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_actoresPrincipales1" android:hint="@string/txt_actoresPrincipales1"
            app:layout_constraintStart_toStartOf="@+id/txt_rating1"
            app:layout_constraintEnd_toEndOf="@+id/txt_rating1" android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_rating1" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_sinopsis1"
            android:hint="@string/txt_sinopsis1"
            app:layout_constraintEnd_toEndOf="@+id/txt_actoresPrincipales1"
            app:layout_constraintStart_toStartOf="@+id/txt_actoresPrincipales1" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_actoresPrincipales1" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.141"/>
    <Button
            android:text="@string/btn_eliminar1"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/btn_eliminar1" android:hint="@string/btn_eliminar1"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toStartOf="@+id/btn_actualizar1"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.709"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/txt_sinopsis1"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.333"/>
    <Button
            android:text="@string/btn_actualizar1"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/btn_actualizar1"
            android:hint="@string/btn_actualizar1" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="40dp" android:layout_marginRight="40dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_sinopsis1" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.333"/>
    <TextView
            android:text="Nombre Película"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.109"
            app:layout_constraintVertical_bias="0.095"/>
    <TextView
            android:text="Año de Lanzamiento"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintEnd_toEndOf="@+id/textView5" app:layout_constraintTop_toBottomOf="@+id/textView5"
            android:layout_marginTop="72dp" app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="Rating"
            android:layout_width="85dp"
            android:layout_height="28dp"
            android:id="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/textView6"
            app:layout_constraintEnd_toEndOf="@+id/textView6" app:layout_constraintTop_toBottomOf="@+id/textView6"
            android:layout_marginTop="64dp" app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="Actores Principales"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            app:layout_constraintEnd_toEndOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/textView7" app:layout_constraintTop_toBottomOf="@+id/textView7"
            android:layout_marginTop="64dp" app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="Sinopsis"
            android:layout_width="70dp"
            android:layout_height="32dp"
            android:id="@+id/textView9"
            app:layout_constraintEnd_toEndOf="@+id/textView8"
            app:layout_constraintStart_toStartOf="@+id/textView8" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/textView8" android:layout_marginTop="64dp"/>
</android.support.constraint.ConstraintLayout>